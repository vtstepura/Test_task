  <tr id="user_<%= user.id %>">
    <td><%= user.username %></td>
    <td><%= user.email %></td>
    <td><%= user.role %></td>
    <td class = "show"><%= link_to 'Show', admin_user_path(user), remote: true %></td>
    <td class="edit" ><%= link_to 'Edit', edit_admin_user_path(user), remote: true %></td>
    <td class="destroy" >
      <%= link_to 'Destroy', admin_user_path(user), method: :delete, data: { remote: true, confirm: 'Are you sure?' } %>
    </td>
  </tr>
